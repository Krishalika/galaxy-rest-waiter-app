{
  "expo": {
    "name": "Galaxy Rest Waiter App",
    "slug": "galaxy-rest-waiter-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/galaxy-rest.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/galaxy-rest.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.krishalika.ReactNativeNavigation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
